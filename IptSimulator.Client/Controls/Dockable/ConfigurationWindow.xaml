<UserControl x:Class="IptSimulator.Client.Controls.Dockable.ConfigurationWindow"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:IptSimulator.Client.Controls"
             xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
             mc:Ignorable="d" 
             d:DesignHeight="800" d:DesignWidth="250"
             d:DataContext="{Binding Source={StaticResource Locator}, Path=TclEditor.Configuration}">
    <Grid>
        <xctk:PropertyGrid AutoGenerateProperties="True"
                           SelectedObject="{Binding}"
                           HideInheritedProperties="True">
            
        </xctk:PropertyGrid>
    </Grid>
</UserControl>
